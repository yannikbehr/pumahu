@article{Scott1994,
  title = {Cyclic {{Activity}} in the {{Crater Lakes}} of {{Waimangu Hydrothermal System}}, {{New Zealand}}},
  author = {Scott, Bradley J.},
  year = {1994},
  journal = {Geothermics},
  volume = {23},
  number = {5-6},
  pages = {555--572},
  issn = {03756505},
  doi = {10.1016/0375-6505(94)90019-1},
  abstract = {Frying Pan and Inferno Crater lakes occupy craters formed on 10 June 1886 during the Tarawera Rift eruption. These lakes are the largest and most spectacular hot springs in New Zealand, and together they display interrelated cyclic variations about a 38 day cycle. The lake occupying Inferno Crater fluctuates from overflow to about 8 m below overflow, although exceptionally the water level has receded below 12 m, which is at least an order of magnitude larger than other known geyser like hot springs. The temperature fluctuates sympathetically with the water level in the range 35.0 to 84.5\textdegree C. Four distinct stages are recognised with the following average parameters; the first lasts 7.7 days, water level and temperature rise respectively 4.7m and 17.3\textdegree C, while the lake volume increases by 23 800 m3. The second lasts 14.7 days, water level rises 3.99 m, the temperature remains steady about 60.7\textdegree C, lake volume increases 22 000 m3. The third is an overflow of 79.1 litre sec-1 for 51.3 hours peaking at 70.6\textdegree C. In the fourth stage, lake level recedes 7.9m decreasing lake volume 45 800 m3 over 15.1 days, and the lake temperature falls 26.4\textdegree C. The discharge of nearby Frying Pan Lake varies inversely with the water level of Inferno Crater Lake by 20.2 {$\pm$} 2.7 litre sec-1 and also responds to a previously unrecognised cycle of about 20 days duration. The temperature of the discharge also responds to the 20 and 38 day cycles, but shows no correlation with discharge volume. Ambient air temperature strongly influences the discharge temperature, which ranges annually between 44.3\textdegree C and 56.6\textdegree C with a mean of 50.1\textdegree C. A slight decrease of about 1.4\textdegree C is apparent between 1972 and 1990. The mean annual discharge from Frying Pan Lake has decreased from 122.2 to 104.1 litre sec-1 between 1972 and 1990. \textcopyright{} 1995.},
  isbn = {0375-6505},
  keywords = {crater lakes,cyclic activity,Frying Pan Lake,hot spring monitoring,Inferno Crater Lake,NZMS 260:U16,Waimangu hydrothermal system},
  file = {/home/behrya/Zotero/storage/K2DQIPPZ/Scott - 1994 - Cyclic activity in the crater lakes of Waimangu hy.pdf}
}

