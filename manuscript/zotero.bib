@article{Deichmann2018a,
  title = {Why Does {{ML}} Scale 1:1 with 0:5 Log {{ES}}?},
  author = {Deichmann, Nicholas},
  year = {2018},
  journal = {Seismological Research Letters},
  volume = {89},
  number = {6},
  pages = {2249--2255},
  issn = {19382057},
  doi = {10.1785/0220180121},
  abstract = {It is well established empirically that ML and 0:5 log ES scale 1:1 over a wide range of magnitudes, in which ML is the local magnitude as determined routinely and ES is an estimate of the seismic energy radiated as S waves. The reason for this is not immediately obvious. In the absence of anelastic attenuation and ignoring the effect of the response of the Wood-Anderson seismometer that is inherent in the definition of ML, theory predicts that, assuming constant stress drop and apparent rupture velocity, the relation between ML and ES should be ML = (2=3) log ES + constant. Theoretical considerations and numerical simulations with synthetic moment-rate functions show that, in reality, the influence of anelastic attenuation on ML and on estimates of ES of small earthquakes as well as the effect of the Wood-Anderson response on ML of larger earthquakes lead to the observed scaling relationship. Because of anelastic attenuation, for small earthquakes with magnitudes below a certain threshold all information about the dynamic properties of the source is irrevocably lost. Consequently, both ML and ES are different measures of earthquake strength for these small events than for larger earthquakes.},
  isbn = {0220180121}
}

