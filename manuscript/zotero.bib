@book{Fang2017,
  title = {Computational {{Statistics}}},
  author = {Fang, Ji-Qian and Jia, Jinzhu},
  year = {2017},
  journal = {Handbook of Medical Statistics},
  doi = {10.1142/9789813148963_0013},
  abstract = {In the analysis of omics data, integrative analysis provides an effective way of pooling information across multiple datasets or multiple correlated responses, and can be more effective than single dataset (response) analysis. Multiple families of integrative analysis methods have been proposed in the literature. The current review focuses on the penalization methods. Special attention is paid to sparse meta-analysis methods that pool summary statistics across datasets, and integrative analysis methods that pool raw data across datasets. We discuss their formulation and rationale. Beyond `standard' penalized selection, we also review contrasted penalization and Laplacian penalization that accommodate finer data structures. The computational aspects, including computational algorithms and tuning parameter selection, are examined. This review concludes with possible limitations and extensions. WIREs Comput Stat 2015, 7:99\textendash 108. doi: 10.1002/wics.1322 For further resources related to this article, please visit the WIREs website. Conflict of interest: The authors have declared no conflicts of interest for this article.},
