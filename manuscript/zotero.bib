@article{Straka2012,
  title = {Truncation Nonlinear Filters for State Estimation with Nonlinear Inequality Constraints},
  author = {Straka, Ond{\v r}ej and Dun{\'i}k, Jind{\v r}ich and {\v S}imandl, Miroslav},
