}

@article{teradaWaterFlowModel2012,
  title = {A Water Flow Model of the Active Crater Lake at {{Aso}} Volcano, {{Japan}}: Fluctuations of Magmatic Gas and Groundwater Fluxes from the Underlying Hydrothermal System},
  shorttitle = {A Water Flow Model of the Active Crater Lake at {{Aso}} Volcano, {{Japan}}},
  author = {Terada, Akihiko and Hashimoto, Takeshi and Kagiyama, Tsuneomi},
  date = {2012-04-01},
  journaltitle = {Bulletin of Volcanology},
  shortjournal = {Bull Volcanol},
