
@article{adamsEvaporationHeatedWater1990,
  title = {Evaporation from Heated Water Bodies: {{Predicting}} Combined Forced plus Free Convection},
  shorttitle = {Evaporation from Heated Water Bodies},
  author = {Adams, E. Eric and Cosler, Douglas J. and Helfrich, Karl R.},
  date = {1990},
  journaltitle = {Water Resources Research},
  volume = {26},
  number = {3},
