}

@article{Nordhausen2009,
  title = {The {{Elements}} of {{Statistical Learning}}: {{Data Mining}}, {{Inference}}, and {{Prediction}}, {{Second Edition}} by {{Trevor Hastie}}, {{Robert Tibshirani}}, {{Jerome Friedman}}},
  author = {Nordhausen, Klaus},
  year = {2009},
