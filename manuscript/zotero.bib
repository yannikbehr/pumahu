@article{diaz-morenoVolumeFlowRate2019,
  title = {Volume {{Flow Rate Estimation}} for {{Small Explosions}} at {{Mt}}. {{Etna}}, {{Italy}}, {{From Acoustic Waveform Inversion}}},
  author = {Diaz-Moreno, A. and Iezzi, A. M. and Lamb, O. D. and Fee, D. and Kim, K. and Zuccarello, L. and De Angelis, S.},
  year = {2019},
  month = oct,
  journal = {Geophysical Research Letters},
  volume = {46},
  number = {20},
  pages = {11071--11079},
  issn = {0094-8276, 1944-8007},
  doi = {10.1029/2019GL084598},
  abstract = {Rapid assessment of the volume and the rate at which gas and pyroclasts are injected into the atmosphere during volcanic explosions is key to effective eruption hazard mitigation. Here, we use data from a dense infrasound network deployed in 2017 on Mt. Etna, Italy, to estimate eruptive volume flow rates (VFRs) during small gas-and-ash explosions. We use a finite-difference time-domain approximation to compute the acoustic Green's functions and perform a full waveform inversion for a multipole source, combining monopole and horizontal dipole terms. The inversion produces realistic estimates of VFR, on the order of 4 \texttimes{} 104 m3/s and well-defined patterns of source directivity. This is the first application of acoustic waveform inversion at Mt. Etna. Our results demonstrate that acoustic waveform inversion is a mature and robust tool for assessment of source parameters and holds potential as a tool to provide rapid estimates of VFR in near real time.},
  langid = {english},
  file = {/home/behrya/Zotero/storage/MT3N7XKZ/Diaz‐Moreno et al. - 2019 - Volume Flow Rate Estimation for Small Explosions a.pdf}
}

