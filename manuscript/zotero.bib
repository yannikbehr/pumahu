@article{lay3DActiveSource,
  title = {{{3D}} Active Source Seismic Imaging of the {{Alpine Fault}} Zone and the {{Whataroa}} Glacial Valley in {{New Zealand}}},
  author = {Lay, Vera and Buske, Stefan and Townend, John and Kellett, Richard and Savage, Martha and Schmitt, Douglas R. and Constantinou, Alexis and Eccles, Jennifer D. and Gorman, Andrew R and Bertram, Malcolm and Hall, Kevin and Lawton, Don and Kofman, Randolph},
  journal = {Journal of Geophysical Research: Solid Earth},
  volume = {n/a},
  number = {n/a},
  pages = {e2021JB023013},
  issn = {2169-9356},
  doi = {10.1029/2021JB023013},
  abstract = {AbstractThe Alpine Fault zone in New Zealand marks a major transpressional plate boundary that is late in its typical earthquake cycle. Understanding the subsurface structures is crucial to understand the tectonic processes taking place. A unique seismic survey including 2D lines, a 3D array, and borehole recordings, has been performed in the Whataroa Valley and provides new insights into the Alpine Fault zone down to {$\sim$}2 km depth at the location of the Deep Fault Drilling Project (DFDP)-2 drill site. Seismic images are obtained by focusing prestack depth migration approaches. Despite the challenging conditions for seismic imaging within a sediment filled glacial valley and steeply dipping valley flanks, several structures related to the valley itself as well as the tectonic fault system are imaged. A set of several reflectors dipping 40\textendash 56\textdegree{} to the southeast are identified in a {$\sim$}600 m wide zone that is interpreted to be the minimum extent of the damage zone. Different approaches image one distinct reflector dipping at {$\sim$}40\textdegree, which is interpreted to be the main Alpine Fault reflector located only {$\sim$}100 m beneath the maximum drilled depth of the DFDP-2B borehole. At shallower depths (z{$<$}0.5 km), additional reflectors are identified as fault segments with generally steeper dips up to 56\textdegree. Additionally, a glacially over-deepened trough with nearly horizontally layered sediments and a major fault (z{$<$}0.5 km) are identified 0.5\textendash 1 km south of the DFDP-2B borehole. Thus, a complex structural environment is seismically imaged and shows the complexity of the Alpine Fault at Whataroa.},
  langid = {english},
  keywords = {Alpine Fault,Deep Fault Drilling Project,prestack depth migration,seismic reflectors,subglacial valley},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2021JB023013},
  file = {/home/behrya/Zotero/storage/FU5IMEBT/Lay et al. - 3D active source seismic imaging of the Alpine Fau.pdf;/home/behrya/Zotero/storage/34K5RF6B/2021JB023013.html}
}

