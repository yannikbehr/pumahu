@article{Bulger2007a,
  title = {Work and {{Personal Life Boundary Management}}: {{Boundary Strength}}, {{Work}}/{{Personal Life Balance}}, and the {{Segmentation-Integration Continuum}}},
  author = {Bulger, Carrie A. and Matthews, Russell A. and Hoffman, Mark E.},
  year = {2007},
