@article{Scott1994,
  title = {Cyclic Activity in the Crater Lakes of {{Waimangu}} Hydrothermal System, {{New Zealand}}},
  author = {Scott, Bradley J.},
  date = {1994},
  journaltitle = {Geothermics},
  volume = {23},
  number = {5-6},
