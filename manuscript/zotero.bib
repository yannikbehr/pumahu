@article{Liu2006a,
  title = {Mixing and Differentiation in the {{Oruanui}} Rhyolitic Magma, {{Taupo}}, {{New Zealand}}: {{Evidence}} from Volatiles and Trace Elements in Melt Inclusions},
  author = {Liu, Yang and Anderson, Alfred T. and Wilson, Colin J N and Davis, Andrew M. and Steele, Ian M.},
  year = {2006},
  journal = {Contributions to Mineralogy and Petrology},
  volume = {151},
