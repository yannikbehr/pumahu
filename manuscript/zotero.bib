@article{Galle2010,
  title = {Network for {{Observation}} of {{Volcanic}} and {{Atmospheric Change}} ({{NOVAC}}) - {{A}} Global Network for Volcanic Gas Monitoring: {{Network}} Layout and Instrument Description},
  author = {Galle, Bo and Johansson, Mattias and Rivera, Claudia and Zhang, Yan and Kihlman, Manne and Kern, Christoph and Lehmann, Thomas and Platt, Ulrich and Arellano, Santiago and Hidalgo, Silvana},
