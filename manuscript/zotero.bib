@article{Lopez-Paz2016,
  title = {Discovering {{Causal Signals}} in {{Images}}},
  author = {{Lopez-Paz}, David and Nishihara, Robert and Chintala, Soumith and Sch{\"o}lkopf, Bernhard and Bottou, L{\'e}on},
