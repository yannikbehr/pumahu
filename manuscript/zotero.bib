@article{illsley-kempVolcanicUnrestTaupo,
  title = {Volcanic Unrest at {{Taup\=o}} Volcano in 2019: {{Causes}}, Mechanisms and Implications},
  shorttitle = {Volcanic Unrest at {{Taup\=o}} Volcano in 2019},
  author = {{Illsley-Kemp}, Finnigan and Barker, Simon J. and Wilson, Colin J. N. and Chamberlain, Calum J. and Hreinsd{\'o}ttir, Sigr{\'u}n and Ellis, Susan and Hamling, Ian J. and Savage, Martha K. and Mestel, Eleanor R. H. and Wadsworth, Fabian B.},
  journal = {Geochemistry, Geophysics, Geosystems},
  volume = {n/a},
  number = {n/a},
  pages = {e2021GC009803},
  issn = {1525-2027},
  doi = {10.1029/2021GC009803},
  abstract = {Taup\=o volcano, New Zealand, is a large caldera volcano that has been highly active through the Holocene. It most recently erupted {$\sim$}1800 years ago but there have been multiple periods of historic volcanic unrest. We use seismological and geodetic analysis to show that in 2019 Taup\=o underwent a period of unrest characterised by increased seismic activity through multiple swarms and was accompanied by ground deformation within the caldera. The earthquakes, which include non-double-couple events, serve to outline an aseismic zone beneath the most recent eruptive vents. This aseismic zone is coincident with an inflating source, based on forward modelling of ground deformation data. We infer that this aseismic and deforming region delineates the location of the present day magma reservoir that is {$\geq$}250 km3 in volume and has a melt fraction of {$>$} 20\textendash 30\%, inhibiting seismic activity. Our analysis shows that the 2019 unrest at Taup\=o was volcanic in nature and origin, demonstrating that this is an active and potentially hazardous volcano, and that improving our monitoring and understanding of its behaviour is important.},
  copyright = {This article is protected by copyright. All rights reserved.},
  langid = {english},
  keywords = {caldera volcano,geodesy,seismology,Taupō,volcano},
  annotation = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2021GC009803},
  file = {/home/behrya/Zotero/storage/KNPP62QD/Illsley-Kemp et al. - Volcanic unrest at Taupō volcano in 2019 Causes, .pdf;/home/behrya/Zotero/storage/4NUPIQ6C/2021GC009803.html}
}

