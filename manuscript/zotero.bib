@article{Kitamura2010c,
  title = {Effects of Pressure on Pore Characteristics and Permeability of Porous Rocks as Estimated from Seismic Wave Velocities in Cores from {{TCDP Hole-A}}},
  author = {Kitamura, Keigo and Takahashi, Miki and Mizoguchi, Kazuo and Masuda, Koji and Ito, Hisao and Song, Sheng Rong},
  year = {2010},
  journal = {Geophysical Journal International},
  volume = {182},
  number = {3},
  pages = {1148--1160},
  issn = {0956540X},
  doi = {10.1111/j.1365-246X.2010.04694.x},
  abstract = {Changes in Vp/Vs (Poisson's ratio) around a fault are related to changes in the fluid transport properties of rocks, which play a significant role in seismogenic processes. Here we report a notable relationship between Vp/Vs and the permeability of porous fault-related rocks (Chelungpu fault, Taiwan) by direct and simultaneous measurement of elastic wave velocities (Vp and Vs) and permeability under increasing effective confining pressure (Peff) up to 25 MPa. Vp and Vs for all samples increased with Peff in the range up to 20 MPa, then were nearly constant as Peff increased to 25 MPa. Most silty sandstones with large proportions of fine-grained material showed positive correlations between Vp/Vs and permeability with rising pressure. On the other hand, well-sorted sandstones showed only slight changes in permeability with respect to Vp/Vs with rising pressure. We infer that grain size distributions, in particular the amount of silt- and clay-size grains, are responsible for the change in permeability with pressure as small particles clog pore networks with increasing Peff, causing the decrease in permeability. These findings may be useful to explain changes in permeability and pore pressure in the deep crust. \textcopyright{} 2010 The Authors Journal compilation \textcopyright{} 2010 RAS.},
  keywords = {Acoustic properties,Fracture and flow,Microstructures,Permeability and porosity}
}

@article{kjaerulffMakingSensitivityAnalysis2013,
  title = {Making {{Sensitivity Analysis Computationally Efficient}}},
  author = {Kj{\ae}rulff, Uffe and {van der Gaag}, Linda C.},
  year = {2013},
  month = jan,
  journal = {arXiv:1301.3868 [cs]},
  eprint = {1301.3868},
  eprinttype = {arxiv},
  primaryclass = {cs},
  url = {http://arxiv.org/abs/1301.3868},
  urldate = {2022-02-28},
  abstract = {To investigate the robustness of the output probabilities of a Bayesian network, a sensitivity analysis can be performed. A one-way sensitivity analysis establishes, for each of the probability parameters of a network, a function expressing a posterior marginal probability of interest in terms of the parameter. Current methods for computing the coefficients in such a function rely on a large number of network evaluations. In this paper, we present a method that requires just a single outward propagation in a junction tree for establishing the coefficients in the functions for all possible parameters; in addition, an inward propagation is required for processing evidence. Conversely, the method requires a single outward propagation for computing the coefficients in the functions expressing all possible posterior marginals in terms of a single parameter. We extend these results to an n-way sensitivity analysis in which sets of parameters are studied.},
  archiveprefix = {arXiv},
  keywords = {Computer Science - Artificial Intelligence},
  file = {/home/behrya/Zotero/storage/H7C22WBF/Kjærulff and van der Gaag - 2013 - Making Sensitivity Analysis Computationally Effici.pdf;/home/behrya/Zotero/storage/AMS3LNKD/1301.html}
}

