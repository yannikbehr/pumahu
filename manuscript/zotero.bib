  author = {{de Moor}, J. M. and Kern, C. and Avard, G. and Muller, C. and Aiuppa, A. and Saballos, A. and Ibarra, M. and LaFemina, P. and Protti, M. and Fischer, T. P.},
  year = {2017},
  journal = {Geochemistry, Geophysics, Geosystems},
  volume = {18},
  number = {12},
  pages = {4437--4468},
  issn = {15252027},
  doi = {10.1002/2017GC007141},
  abstract = {This work presents a new database of SO2 and CO2 fluxes from the Southern Central American Volcanic Arc (SCAVA) for the period 2015\textendash 2016. We report {$\sim$}300 SO2 flux measurements from 10 volcanoes and gas ratios from 11 volcanoes in Costa Rica and Nicaragua representing the most extensive available assessment of this {$\sim$}500 km arc segment. The SO2 flux from SCAVA is estimated at 6,240 {$\pm$} 1,150 T/d, about a factor of three higher than previous estimations (1972\textendash 2013). We attribute this increase in part to our more complete assessment of the arc. Another consideration in interpreting the difference is the context of increased volcanic activity, as there were more eruptions in 2015\textendash 2016 than in any period since {$\sim$}1980. A potential explanation for increased degassing and volcanic activity is a change in crustal stress regime (from compression to extension, opening volcanic conduits) following two large (Mw {$>$} 7) earthquakes in the region in 2012. The CO2 flux from the arc is estimated at 22,500 {$\pm$} 4,900 T/d, which is equal to or greater than estimates of C input into the SCAVA subduction zone. Time-series data sets for arc degassing need to be improved in temporal and spatial coverage to robustly constrain volatile budgets and tectonic controls. Arc volatile budgets are strongly influenced by short-lived degassing events and arc systems likely display significant short-term variations in volatile output, calling for expansion of nascent geochemical monitoring networks to achieve spatial and temporal coverage similar to traditional geophysical networks.},
  keywords = {central America,eruptions,gas monitoring,magma tectonic interactions,volatile budgets,volcanic degassing}
}

