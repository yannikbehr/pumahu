@article{Society1975,
  title = {Theoretical Basis of Some Empirical Relations in Seismology},
  author = {Hiroo Kanamori and Don L. Anderson},
  year = {1975},
  journal = {Bulletin of the Seismological Society of America},
  volume = {65},
  number = {5},
  pages = {1073--1095},
  issn = {1943-3573},
  abstract = {Empirical relations involving seismic moment Mo, magnitude MS, energy ES and fault dimension L (or area S) are discussed on the basis of an extensive set of earthquake data (MS {$\geqq$} 6) and simple crack and dynamic dislocation models. The relation between log S and log Mo is remarkably linear (slope {$\sim$} 2/3) indicating a constant stress drop {$\Delta\sigma$}; {$\Delta\sigma$} = 30, 100 and 60 bars are obtained for inter-plate, intra-plate and ``average'' earthquakes, respectively. Except for very large earthquakes, the relation MS {$\sim$} (2/3) log Mo {$\sim$} 2 log L is established by the data. This is consistent with the dynamic dislocation model for point dislocation rise times and rupture times of most earthquakes. For very large earthquakes MS {$\sim$} (1/3) log Mo {$\sim$} log L {$\sim$} (1/3) log ES. For very small earthquakes MS {$\sim$} log Mo {$\sim$} 3 log L {$\sim$} log ES. Scaling rules are assumed and justified. This model predicts log ES {$\sim$} 1.5 MS {$\sim$} 3 log L which is consistent with the Gutenberg-Richter relation. Since the static energy is proportional to {$\overline{\sigma}$}L3, where {$\overline{\sigma}$} is the average stress, this relation suggests a constant apparent stress {$\eta\overline{\sigma}$} where {$\eta$} is the efficiency. The earthquake data suggest {$\eta\overline{\sigma}\sptilde$}0.5 {$\Delta\sigma$}. These relations lead to log S {$\sim$} MS consistent with the empirical relation. This relation together with a simple geometrical argument explains the magnitude-frequency relation log N {$\sim$} - MS.},
  keywords = {area,dimensions,duration,dynamic,earthquakes,energy,faults,frequency,magnitude,relation,seismic moment,static,stress,theoretical studies},
