@article{seydouxClusteringEarthquakeSignals2020,
  title = {Clustering Earthquake Signals and Background Noises in Continuous Seismic Data with Unsupervised Deep Learning},
  author = {Seydoux, L{\'e}onard and Balestriero, Randall and Poli, Piero and de Hoop, Maarten and Campillo, Michel and Baraniuk, Richard},
