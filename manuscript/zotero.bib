@article{teradaPreciseRemotemonitoringTechnique2008,
  title = {Precise Remote-Monitoring Technique of Water Volume and Temperature of a Crater Lake in {{Aso}} Volcano, {{Japan}}: Implications for a Sensitive Window of a Volcanic Hydrothermal System},
  shorttitle = {Precise Remote-Monitoring Technique of Water Volume and Temperature of a Crater Lake in {{Aso}} Volcano, {{Japan}}},
  author = {Terada, Akihiko and Hashimoto, Takeshi and Kagiyama, Tsuneomi and Sasaki, Hisashi},
  year = {2008},
  month = jun,
  journal = {Earth, Planets and Space},
  volume = {60},
  number = {6},
  pages = {705--710},
  issn = {1880-5981},
  doi = {10.1186/BF03353134},
  abstract = {A high-resolution Digital Surface Model and a commercial digital camera have enabled precise and continuous monitoring of the crater lake at Aso volcano. From July 2006 onwards, infrared (IR) thermometry has been used with this system, enabling more accurate measurements of lake volume and temperature based on simple and intensive observations than has been possible in any other previous studies. The heat discharge remained largely constant at approximately 220 MW, with the exception of an abrupt increase to 280 MW that coincided with a rapid decrease in the water level in August 2007. Simultaneously, an increase in temperature at a shallow depth was suggested by other observations. The crater lake was found to respond to even slight changes in volcanic fluid supply, which can be well quantified by our method. Thus, a crater lake can be monitored more precisely than subaerial fumaroles whose energy estimation is often accompanied by large uncertainties. Our monitoring technique of a crater lake provides information on the subsurface hydrothermal system beneath it, for which any in-situ measurements are practically impossible.},
  langid = {english},
  file = {/home/behrya/Zotero/storage/D5UDJYXJ/Terada et al. - 2008 - Precise remote-monitoring technique of water volum.pdf}
}

@article{teradaWaterFlowModel2012,
  title = {A Water Flow Model of the Active Crater Lake at {{Aso}} Volcano, {{Japan}}: Fluctuations of Magmatic Gas and Groundwater Fluxes from the Underlying Hydrothermal System},
  shorttitle = {A Water Flow Model of the Active Crater Lake at {{Aso}} Volcano, {{Japan}}},
  author = {Terada, Akihiko and Hashimoto, Takeshi and Kagiyama, Tsuneomi},
  year = {2012},
  month = apr,
  journal = {Bulletin of Volcanology},
  volume = {74},
  number = {3},
  pages = {641--655},
  issn = {1432-0819},
  doi = {10.1007/s00445-011-0550-4},
  abstract = {The first crater of Nakadake, peak of Aso volcano, Japan, contains a hot water lake that shows interesting variations in water level and temperature. These variations were discovered by precise, continuous observations of the lake independent of precipitation. We developed a numerical model of a hot crater lake and compared with observational data for the period from July 2006 to January 2009. The numerical model revealed seasonal changes in mass flux (75\textendash 132~kg/s) and enthalpy (1,840\textendash 3,030~kJ/kg) for the fluid supplied to the lake. The relation between the enthalpy and mass flux indicates that the bottom input fluid is a mixture of high- and low-temperature fluids. Assuming a mixture of high-temperature steam at 800\textdegree C and liquid water at 100\textdegree C, we evaluated the liquid and steam fluxes. The liquid water flux shows a seasonal increase lagging behind the rainy season by 2~months, suggesting that the liquid water is predominantly groundwater. The fluctuation pattern in the flux of the high-temperature steam shows a relation with the amplitude of volcanic tremor, suggesting that heating of the hydrothermal system drives the tremor. Consequently, precise observations of a hot crater lake represent a potential method of monitoring volcanic hydrothermal systems in the shallow parts of the volcanoes.},
  langid = {english},
  file = {/home/behrya/Zotero/storage/4NGFBGQG/Terada et al. - 2012 - A water flow model of the active crater lake at As.pdf}
}

