@article{christensonCyclicProcessesFactors2010,
  title = {Cyclic Processes and Factors Leading to Phreatic Eruption Events: {{Insights}} from the 25 {{September}} 2007 Eruption through {{Ruapehu Crater Lake}}, {{New Zealand}}},
