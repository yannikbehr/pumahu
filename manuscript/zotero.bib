@article{wangTidalModulationSeismicity2022,
  title = {Tidal Modulation of Seismicity at the {{Coso}} Geothermal Field},
  author = {Wang, Wei and Shearer, Peter M. and Vidale, John E. and Xu, Xiaohua and Trugman, Daniel T. and Fialko, Yuri},
  year = {2022},
