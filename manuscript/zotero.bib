@article{broschDestructivenessPyroclasticSurges2021,
  title = {Destructiveness of Pyroclastic Surges Controlled by Turbulent Fluctuations},
  author = {Brosch, Ermanno and Lube, Gert and Cerminara, Matteo and {Esposti-Ongaro}, Tomaso and Breard, Eric C. P. and Dufek, Josef and Sovilla, Betty and Fullard, Luke},
